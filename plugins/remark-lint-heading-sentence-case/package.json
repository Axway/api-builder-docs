{
	"name": "remark-lint-heading-sentence-case",
	"version": "1.0.0",
	"description": "Check for heading sentence case",
	"main": "index.js",
	"scripts": {
		"test": "mocha test.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Trott/remark-lint-prohibited-strings.git"
	},
	"keywords": [
		"remark",
		"lint",
		"markdown",
		"md",
		"strings"
	],
	"author": "Rich Trott",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Trott/remark-lint-prohibited-strings/issues"
	},
	"homepage": "https://github.com/Trott/remark-lint-prohibited-strings#readme",
	"dependencies": {
		"debug": "^4.3.2",
		"escape-string-regexp": "^4.0.0",
		"unified-lint-rule": "^1.0.2",
		"unist-util-position": "^3.1.0",
		"unist-util-visit": "^2.0.0",
		"vfile-location": "^3.0.1"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/git": "^9.0.0",
		"c8": "^7.0.0",
		"chai": "^4.3.4",
		"mocha": "^9.1.0",
		"remark": "^13.0.0",
		"remark-lint": "^8.0.0",
		"semantic-release": "^17.2.1",
		"standard": "^16.0.0",
		"tape": "^5.0.0",
		"vfile": "^4.0.1"
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/changelog",
				{
					"changelogFile": "CHANGELOG"
				}
			],
			"@semantic-release/npm",
			[
				"@semantic-release/git",
				{
					"assets": [
						"CHANGELOG",
						"package.json"
					],
					"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
				}
			]
		]
	}
}
